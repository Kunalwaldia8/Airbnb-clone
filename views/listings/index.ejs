<% layout("/layouts/boilerplate") -%>
    
    <div class="mt-3">

    
    <h3>All Listings</h3>
  </div>
        <div class=" row row-cols-lg-3 row-cols-md-2 row-cols-sm-1 row-cols-xs-1">           
        <% for (let element of listing) {%>
          <a href="/listings/<%= element._id %>" class="linking-list">
            <div class="card listings-card col " >
                <img src=<%=element.image%> class="card-img-top" alt="image" style="height:20rem">
                <div class="card-img-overlay"></div>
                <div class="card-body">
                  <p class="card-text">
                    <b> <%= element.title %> </b><br>
                    &#8377; <%= element.price %> <br>
                  </p>
                </div>
              </div>
          </a>
              <% } %>
            </div>



